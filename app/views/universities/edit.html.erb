<div class="col-md-6 col-md-offset-3">

	<div class="new-university-form">


		<h1>Edit Uni</h1>
		<%= simple_form_for @university do |f| %>
		<%= f.select :category_id, @categories %>
		<%= f.file_field :uni_image %>
		<%= f.input :name, label: "university Name"%>
		<%= f.input :description %>
		<%= f.input :location %>
		<%= f.button :submit ,%>

		<% end %>

	</div>

</div>